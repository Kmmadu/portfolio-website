{
    "compilerOptions": {
      "jsx": "preserve",
      "module": "esnext",
      "target": "es5",
      "lib": [
        "dom",
        "dom.iterable",
        "esnext"
      ],
      "allowJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true,
      "incremental": true,
      "strict": false,
      "baseUrl": "src",
      "paths": {
        "@/*": ["*"]
      },
      "plugins": [
        {
          "name": "next"
        }
      ]
    },
    "include": [
      "**/*.ts",
      "**/*.tsx",
      "next-env.d.ts",
      ".next/types/**/*.ts"
    ],
    "exclude": [
      "node_modules"
    ]
  }
  